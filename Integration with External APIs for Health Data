import requests

def get_external_health_data(patient_id):
    # Example external API URL
    api_url = f"https://api.healthdata.com/patients/{patient_id}"
    response = requests.get(api_url)
    
    if response.status_code == 200:
        return response.json()
    else:
        return {"error": "Failed to fetch data"}

# Fetch health data for a specific patient
patient_id = 'patient1'
data = get_external_health_data(patient_id)
print(data)
